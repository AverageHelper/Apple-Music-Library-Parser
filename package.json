{
	"name": "apple-music-library-parser",
	"version": "0.0.1",
	"description": "A Node.js project to parse the Library.xml file you get from Apple Music into a nice UI without messing up your existing library.",
	"main": "./lib/main.js",
	"type": "module",
	"files": [
		"lib/"
	],

	"scripts": {
		"prebuild": "npm run export-version && npm run lint",
		"build": "tsc -p tsconfig.prod.json",
		"build:clean": "rm -rf lib && npm run build",
		"start": "node . --",
		"test": "jest --selectProjects src --verbose --coverage",
		"test:watch": "npm run test -- --watch --coverage=false",
		"export-version": "genversion ./src/version.ts -es",
		"lint": "eslint . --ext .ts",
		"lint:fix": "eslint . --ext .ts --fix"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/AverageHelper/Apple-Music-Library-Parser.git"
	},
	"keywords": [
		"node",
		"apple-music",
		"library",
		"parse",
		"xml"
	],
	"author": "Average Helper",
	"license": "GPL-3.0",
	"bugs": {
		"url": "https://github.com/AverageHelper/Apple-Music-Library-Parser/issues"
	},
	"homepage": "https://github.com/AverageHelper/Apple-Music-Library-Parser#readme",
	"dependencies": {
		"inquirer": "^8.2.0",
		"lodash": "^4.17.21",
		"ora": "^6.0.1",
		"plist": "^3.0.4",
		"source-map-support": "^0.5.19",
		"yargs": "^17.0.1"
	},
	"devDependencies": {
		"@types/inquirer": "^8.1.3",
		"@types/jest": "^27.4.0",
		"@types/lodash": "^4.14.178",
		"@types/node": "^16.4.2",
		"@types/plist": "^3.0.2",
		"@types/source-map-support": "^0.5.4",
		"@typescript-eslint/eslint-plugin": "^5.1.0",
		"@typescript-eslint/parser": "^5.1.0",
		"eslint": "^7.32.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-config-strictest": "^0.3.1",
		"eslint-plugin-jest": "^25.2.2",
		"eslint-plugin-node": "^11.1.0",
		"eslint-plugin-prettier": "^4.0.0",
		"eslint-plugin-promise": "^5.1.1",
		"eslint-plugin-unicorn": "^37.0.1",
		"genversion": "^3.0.1",
		"jest": "^27.3.1",
		"jest-environment-node": "^27.3.1",
		"jest-extended": "^1.2.0",
		"prettier": "^2.3.2",
		"ts-jest": "^27.0.7",
		"typescript": "^4.4.3"
	}
}
